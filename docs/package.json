    "@vuepress/plugin-pwa": "^2.0.0-rc.24",
    "@vuepress/plugin-google-analytics": "^2.0.0-rc.24",
    "vuepress-plugin-code-copy": "^1.0.6",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-footnote": "^3.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "sass": "^1.69.0"
  },
  "engines": {
    "node": ">=20.9.0",

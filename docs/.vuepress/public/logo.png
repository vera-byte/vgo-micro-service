<svg xmlns="http://www.w3.org/2000/svg" width="120" height="120" viewBox="0 0 120 120">
  <defs>
    <linearGradient id="logoGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3eaf7c;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#4abf8a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#5ccf96;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-color="rgba(0,0,0,0.3)"/>
    </filter>
  </defs>
  
  <!-- 主背景圆形 -->
  <circle cx="60" cy="60" r="55" fill="url(#logoGrad)" filter="url(#shadow)"/>
  
  <!-- 内部装饰圆环 -->
  <circle cx="60" cy="60" r="45" fill="none" stroke="rgba(255,255,255,0.2)" stroke-width="1"/>
  <circle cx="60" cy="60" r="35" fill="none" stroke="rgba(255,255,255,0.1)" stroke-width="1"/>
  
  <!-- VGO 主文字 -->
  <text x="60" y="75" font-family="Arial, sans-serif" font-size="28" font-weight="bold" text-anchor="middle" fill="white">VGO</text>
  
  <!-- 副标题 -->
  <text x="60" y="90" font-family="Arial, sans-serif" font-size="8" text-anchor="middle" fill="rgba(255,255,255,0.8)">MICROSERVICE</text>
  
  <!-- 顶部装饰图标 -->
  <g transform="translate(60,30)">
    <!-- 盾牌图标表示安全 -->
    <path d="M-8,-8 L-8,8 L0,12 L8,8 L8,-8 Z" fill="white" opacity="0.9"/>
    <path d="M-6,-6 L-6,6 L0,9 L6,6 L6,-6 Z" fill="url(#logoGrad)"/>
    <!-- 锁图标 -->
    <rect x="-3" y="-2" width="6" height="4" fill="white" rx="1"/>
    <circle cx="0" cy="-4" r="2" fill="none" stroke="white" stroke-width="1"/>
  </g>
  
  <!-- 底部装饰线条 -->
  <g transform="translate(60,100)">
    <path d="M-20,0 L-10,-3 L0,0 L10,-3 L20,0" stroke="white" stroke-width="2" fill="none" opacity="0.6" stroke-linecap="round"/>
  </g>
  
  <!-- 左右装饰元素 -->
  <g transform="translate(25,60)">
    <circle r="3" fill="white" opacity="0.4"/>
    <circle r="1.5" fill="white" opacity="0.8"/>
  </g>
  <g transform="translate(95,60)">
    <circle r="3" fill="white" opacity="0.4"/>
    <circle r="1.5" fill="white" opacity="0.8"/>
  </g>
  
  <!-- 微服务连接线条装饰 -->
  <g opacity="0.3">
    <line x1="30" y1="40" x2="90" y2="40" stroke="white" stroke-width="1" stroke-dasharray="2,2"/>
    <line x1="30" y1="80" x2="90" y2="80" stroke="white" stroke-width="1" stroke-dasharray="2,2"/>
    <line x1="40" y1="30" x2="40" y2="90" stroke="white" stroke-width="1" stroke-dasharray="2,2"/>
    <line x1="80" y1="30" x2="80" y2="90" stroke="white" stroke-width="1" stroke-dasharray="2,2"/>
  </g>
</svg>